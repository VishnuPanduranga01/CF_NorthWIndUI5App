{
    "welcomeFile":"ui/index.html",
    "authenticationMethod":"none",
    "routes":[
        {
            "source":"^/ui/(.*)$",
            "target":"$1",
            "localDir":"./ui/webapp",
            "authenticationType": "none"
        },
        {
            "source": "^/resources/(.*)$",
            "target": "/resources/$1",
            "destination": "ui5",
            "authenticationType": "none"
        },
        {
            "source": "^/v2/northwind/(.*)$",
            "target": "/v2/northwind/$1",
            "destination": "northWind",
            "authenticationType": "none",
            "csrfProtection": false
        }
    ]

}